{
  "buildCommand": "npm run build",
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/sps/content/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "OPENAI_REALTIME_MODEL": "gpt-realtime-mini-2025-10-06"
  }
}
