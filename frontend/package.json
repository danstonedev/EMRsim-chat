{
  "name": "emrsim-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "predev": "node scripts/scan-animations.mjs",
    "dev": "vite",
    "prebuild": "node scripts/scan-animations.mjs",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
  "pretest": "node scripts/scan-animations.mjs",
  "test": "vitest run --maxWorkers=50%",
  "pretest:viewer": "node scripts/scan-animations.mjs",
  "test:viewer": "vitest --run src/pages/components/viewer/__tests__/mixerController.resume.spec.ts",
  "test:viewer:watch": "vitest src/pages/components/viewer/__tests__/mixerController.resume.spec.ts",
    "test:watch": "vitest",
    "test:coverage": "vitest run --maxWorkers=50% --coverage",
  "lint": "eslint src --ext .ts,.tsx",
  "lint:report": "eslint src --ext .ts,.tsx -f json -o eslint-report.json",
  "lint:fix": "eslint src --ext .ts,.tsx --fix",
  "lint:all": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css}\"",
    "perf:baseline": "node scripts/render-metrics-baseline.mjs"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.20",
    "@react-three/drei": "^9.88.0",
    "@react-three/fiber": "^8.15.0",
    "docx": "^9.5.1",
    "file-saver": "^2.0.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "socket.io-client": "^4.8.1",
    "three": "^0.162.0",
    "three-stdlib": "^2.29.4"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.0.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/file-saver": "^2.0.7",
  "@types/react": "^18.3.11",
  "@types/react-dom": "^18.3.1",
    "@types/three": "^0.162.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.0",
    "jsdom": "^24.0.0",
    "msw": "^2.4.2",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.46.0",
    "vite": "^5.4.3",
    "vitest": "^1.6.1"
  }
}
